fact = |n m| {
    d = 2;

    cont = 0;

    while n > 1 & m > 1 {
        if n % d == 0 & m % d == 0 {
            cont = cont + 1;
        }

        while n % d == 0 {
            n = n / d;
        }

        while m % d == 0 {
            m = m / d;
        }

        d = d + 1;
    }

    ret cont;
};

print(factori(750, 490));
